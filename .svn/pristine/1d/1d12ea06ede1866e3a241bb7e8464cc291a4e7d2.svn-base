<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<html>
<head>
    <title>Title</title>
</head>
<body>
    <section>
	        <div class="panel-body">
	            <div class="line line-dashed line-lg pull-in"></div>
	            <div class="form-group">
	                <label class="col-sm-2 control-label">代金券ID</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="cashCouponId" value="${payGeneral.cashCouponId}" disabled="disabled">
	            	</div>
	            </div>
	            <div class="form-group">
	                <label class="col-sm-2 control-label">用户ID</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="userId" value="${payGeneral.userId}" disabled="disabled">
	            	</div>
	            </div>
	            <div class="form-group">
	                <label class="col-sm-2 control-label">来源类别</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="origin" value="${payGeneral.origin}" disabled="disabled">
	            	</div>
	            </div>
	            <div class="form-group" id="input" >
	                <label class="col-sm-2 control-label">标题</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="title" value="${payGeneral.title}" disabled="disabled">
	            	</div>
	            </div>
	            <div class="form-group">
	                <label class="col-sm-2 control-label">面额</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name=denomination value="${payGeneral.denomination}" disabled="disabled">
	            	</div>
	            </div>
	             <div class="form-group">
	                <label class="col-sm-2 control-label">余额</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="couponMoney" value="${payGeneral.couponMoney}" disabled="disabled">
	            	</div>
	            </div>
	             <div class="form-group">
	                <label class="col-sm-2 control-label">生成时间</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="generateTime" value="${payGeneral.generateTimeShow}" disabled="disabled">
	            	</div>
	            </div>
	             <div class="form-group">
	                <label class="col-sm-2 control-label">启用时间</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="enableTime" value="${payGeneral.enableTimeShow}" disabled="disabled">
	            	</div>
	            </div>
	             <div class="form-group">
	                <label class="col-sm-2 control-label">失效时间</label>
	                <div class="col-sm-10">															   
	                	<input type="text" class="form-control" name="disableTime" value="${payGeneral.disableTimeShow}" disabled="disabled">
	            	</div>
	            </div>
	             <div class="form-group">
	                <label class="col-sm-2 control-label">券状态</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="couponSatus" value="${payGeneral.couponSatus}" disabled="disabled">
	            	</div>
	            </div>
	           <%--   <div class="form-group">
	                <label class="col-sm-2 control-label">冻结时间</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="overdueDate" value="${payGeneral.freezeAliveShow}" disabled="disabled">
	            	</div>
	            </div> --%>
	         </div>
    </section>
</body>
</html>
