<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>

<html>
<head>
    <title>Title</title>
    
</head>
<body>
    <section>
	        <div class="panel-body">
	            <div class="form-group">
	                <label class="col-sm-2 control-label">购买人</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="purchaser" value="${record.purchaser}" disabled="disabled">
	            	</div>
	            </div>
	            <div class="form-group">
	                <label class="col-sm-2 control-label">规则标题</label>
	                <div class="col-sm-10">
	                      <input type="text" class="form-control" name="title" value="${record.title}"
	                             data-bv-notempty="true" data-bv-notempty-message="标题不能为空" disabled="disabled">
	                </div>
	            </div>
	            
	            <div class="line line-dashed line-lg pull-in"></div>
	            <div class="form-group">
	                <label class="col-sm-2 control-label">购买金额</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="buyAmount" value="${record.buyAmount}" disabled="disabled">
	            	</div>
	            </div>
	            <div class="line line-dashed line-lg pull-in"></div>
	            <div class="form-group">
	                <label class="col-sm-2 control-label">面值</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="faceValue" value="${record.faceValue}" disabled="disabled">
	                	<input type="hidden" class="form-control" name="id" value="${record.id}">
	            	</div>
	            </div>
	            <div class="line line-dashed line-lg pull-in"></div>
	            <div class="form-group" id="input" >
	                <label class="col-sm-2 control-label">购买时间</label>
	                <div class="col-sm-10">
	                	<input type="text" class="form-control" name="buyTime" value="${record.searchStartTime}" disabled="disabled">
	            	</div>
	            </div>
	        </div>
    </section>
</body>
</html>
